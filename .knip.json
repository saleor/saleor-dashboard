{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "rules": {
    "types": "off",
    "enumMembers": "off",
    "files": "off",
    "exports": "off",
    "duplicates": "off",
    "classMembers": "error",
    "nsTypes": "error",
    "unresolved": "error",
    "nsExports": "error",
    "unlisted": "error",
    "binaries": "error",
    "optionalPeerDependencies": "error",
    "dependencies": "error",
    "devDependencies": "error"
  },
  "ignore": [
    "src/graphql/types.generated.ts",
    "src/graphql/typePolicies.generated.ts",
    "src/graphql/hooks.generated.ts",
    "playwright/**",
    "src/fragments/**",
    "testUtils/**",
    "scripts/**",
    "assets/**",
    "**/*.js",
    "**/queries.ts",
    "**/fixtures.ts",
    "**/mutations.ts",
    "**/filters.ts",
    "src/searches/**/*.ts",
    "src/fixtures",
    "src/utils/errors/product.ts",
    "src/orders/components/OrderSummaryCard/utils.ts",
    "src/misc.ts",
    "*mock*.ts",
    "src/orders/components/OrderReturnPage/components/ItemsCard/ItemsCard.tsx",
    "src/hooks/makeMutation.ts",
    "src/components/DemoBanner/constants.ts",
    "src/components/ConditionalFilter/UI/types.ts",
    "Savebar.tsx"
  ],
  "ignoreDependencies": [
    "@graphql-codegen/typescript-apollo-client-helpers",
    "@graphql-codegen/typescript-operations",
    "@graphql-codegen/typescript-react-apollo",
    "@graphql-codegen/add",
    "@graphql-codegen/fragment-matcher",
    "@graphql-codegen/import-types-preset",
    "@graphql-codegen/typescript",
    "@material-ui/styles",
    "keycode",
    "tslib",
    "@swc/jest",
    "@types/lodash-es",
    "@types/webappsec-credential-management",
    "core-js",
    "identity-obj-proxy",
    "moment",
    "history",
    "react-transition-group",
    "@radix-ui/react-radio-group",
    "@react-editor-js/core",
    "@radix-ui/react-portal",
    "@graphql-codegen/schema-ast"
  ],
  "ignoreBinaries": ["testmo", "saleor", "lockfile-lint", "prettier", "husky", "jest"]
}
