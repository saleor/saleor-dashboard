{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "rules": {
    "types": "error",
    "enumMembers": "off",
    "files": "error",
    "exports": "off",
    "duplicates": "off",
    "classMembers": "off",
    "nsTypes": "off",
    "unresolved": "error",
    "nsExports": "off",
    "unlisted": "error",
    "binaries": "off",
    "optionalPeerDependencies": "error",
    "dependencies": "error",
    "devDependencies": "error"
  },
  "entry": [
    "src/index.tsx!",
    "src/index.css!",
    "src/index.html!",
    "src/**/queries.ts",
    "src/**/mutations.ts",
    "src/**/fragments/*.ts",
    "src/searches/*.ts",
    "src/**/fixtures.ts",
    "codegen.ts",
    "scripts/**"
  ],
  "project": [
    "**/*.{js,ts,tsx,jsx}"
  ],
  "jest": {
    "entry": [
      "**/*.test.{ts,tsx}",
      "testUtils/**/*.{ts,tsx}"
    ]
  },
  "ignore": [
    "playwright/**",
    "**/*.generated.ts"
  ],
  "ignoreDependencies": [
    "@graphql-codegen/typescript-apollo-client-helpers",
    "@graphql-codegen/typescript-operations",
    "@graphql-codegen/typescript-react-apollo",
    "@graphql-codegen/add",
    "@graphql-codegen/fragment-matcher",
    "@graphql-codegen/import-types-preset",
    "@graphql-codegen/typescript",
    "@material-ui/styles",
    "keycode",
    "tslib",
    "@swc/jest",
    "@types/lodash-es",
    "@types/webappsec-credential-management",
    "core-js",
    "identity-obj-proxy",
    "moment",
    "history",
    "react-transition-group",
    "@radix-ui/react-radio-group",
    "@react-editor-js/core",
    "@radix-ui/react-portal",
    "@graphql-codegen/schema-ast",
    "@sentry/cli",
    "mocha",
    "mochawesome",
    "mochawesome-report-generator",
    "mochawesome-merge",
    "playwright-ctrf-json-reporter",
    "rollup-plugin-polyfill-node",
    "vite-plugin-html",
    "@vitejs/plugin-react-swc",
    "@sentry/vite-plugin",
    "code-inspector-plugin",
    "faker",
    "@types/faker",
    "sort-package-json",
    "downshift"
  ]
}
