{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "rules": {
    "types": "warn",
    "enumMembers": "off",
    "files": "warn",
    "exports": "warn",
    "duplicates": "warn",
    "classMembers": "warn",
    "nsTypes": "warn",
    "unresolved": "warn",
    "nsExports": "warn",
    "unlisted": "warn",
    "binaries": "off",
    "optionalPeerDependencies": "error",
    "dependencies": "error",
    "devDependencies": "error"
  },
  "entry": [
    "src/index.tsx!",
    "assets/**!",
    "src/**/queries.ts",
    "src/**/mutations.ts",
    "src/**/fragments/*.ts",
    "src/searches/*.ts",
    "src/**/fixtures.ts",
    "scripts/**"
  ],
  "project": ["**/*.{js,ts,tsx,jsx}"],
  "jest": true,
  "vite": true,
  "eslint": true,
  "playwright": true,
  "lint-staged": true,
  "graphql-codegen": true,
  "dotenv": true,
  "storybook": true,
  "typescript": true,
  "prettier": true,
  "ignore": ["playwright/**", "**/*.generated.ts", "apollo.config.js"],
  "ignoreDependencies": [
    "keycode",
    "tslib",
    "@types/lodash-es",
    "@types/webappsec-credential-management",
    "core-js",
    "moment",
    "history",
    "react-transition-group",
    "@sentry/cli",
    "@react-editor-js/core",
    "playwright-ctrf-json-reporter",
    "downshift"
  ]
}
