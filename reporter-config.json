{
  "reporterEnabled": "mocha-junit-reporter, mochawesome",

  "mochaJunitReporterReporterOptions": {
    "mochaFile": "cypress/reports/junit/results-[hash].xml"
  },
  "reporterOptions": {
    "reportDir": "cypress/reports/mocha",
    "screenshotOnRunFailure": true,
    "overwrite": false,
    "quite": true,
    "html": false,
    "json": true
  }
}
